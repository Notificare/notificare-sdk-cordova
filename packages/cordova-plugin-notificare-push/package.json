{
  "name": "cordova-plugin-notificare-push",
  "version": "3.0.0-beta.2",
  "description": "Notificare Push Cordova Plugin.",
  "author": "Notificare <info@notifica.re> (https://notificare.com)",
  "license": "MIT",
  "homepage": "https://github.com/notificare/notificare-sdk-cordova#readme",
  "repository": {
    "url": "https://github.com/Notificare/notificare-sdk-cordova"
  },
  "bugs": {
    "url": "https://github.com/Notificare/notificare-sdk-cordova/issues"
  },
  "keywords": [
    "notificare",
    "ecosystem:cordova",
    "cordova-android",
    "cordova-ios"
  ],
  "main": "build/notificare-push.bundle.js",
  "types": "build/js/index.d.ts",
  "files": [
    "src",
    "build",
    "plugin.xml",
    "CHANGELOG.md",
    "MIGRATION.md"
  ],
  "scripts": {
    "prepare": "run-s build",
    "prepack": "run-s symlinks-to-files",
    "postpack": "run-s restore-symlinks",
    "build": "run-s clean build:typescript build:bundle",
    "build:typescript": "tsc -p tsconfig.json",
    "build:bundle": "rollup --config",
    "clean": "rimraf build",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "symlinks-to-files": "rm README.md LICENSE CHANGELOG.md; cp ../README.md ../LICENSE ../CHANGELOG.md .",
    "restore-symlinks": "rm README.md LICENSE CHANGELOG.md; ln -s ../README.md ../LICENSE ../CHANGELOG.md ."
  },
  "cordova": {
    "id": "cordova-plugin-notificare-push",
    "platforms": [
      "android",
      "ios"
    ]
  },
  "engines": {
    "cordovaDependencies": {
      ">=3.0.0": {
        "cordova": ">= 9.0.0",
        "cordova-android": ">=8.0.0",
        "cordova-ios": ">=5.0.0"
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@types/cordova": "^0.0.34",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "cordova-plugin-notificare": "../notificare",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "husky": "^4.3.5",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.35.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.29.0",
    "typescript": "^4.1.2"
  },
  "peerDependencies": {
    "cordova-plugin-notificare": "../notificare"
  }
}
