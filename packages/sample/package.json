{
  "name": "sample",
  "private": "true",
  "displayName": "HelloCordova",
  "version": "1.0.0",
  "description": "A sample Apache Cordova application that responds to the deviceready event.",
  "main": "index.js",
  "scripts": {
    "cordova:remove-platforms": "rm -rf platforms plugins",
    "cordova:add-android": "cordova platform add android; cd platforms/android; gradle wrapper",
    "cordova:add-ios": "cordova platform add ios; cd platforms/ios; pod install"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Apache Cordova Team",
  "license": "Apache-2.0",
  "devDependencies": {
    "cordova-android": "^10.1.1",
    "cordova-ios": "^6.2.0",
    "cordova-plugin-notificare": "^3.0.0-beta.2",
    "cordova-plugin-notificare-assets": "^3.0.0-beta.2",
    "cordova-plugin-notificare-geo": "^3.0.0-beta.2",
    "cordova-plugin-notificare-inbox": "^3.0.0-beta.2",
    "cordova-plugin-notificare-loyalty": "^3.0.0-beta.2",
    "cordova-plugin-notificare-push": "^3.0.0-beta.2",
    "cordova-plugin-notificare-push-ui": "^3.0.0-beta.2",
    "cordova-plugin-notificare-scannables": "^3.0.0-beta.2",
    "cordova-plugin-whitelist": "^1.3.4",
    "ios-deploy": "^1.11.4"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-notificare": {},
      "cordova-plugin-notificare-assets": {},
      "cordova-plugin-notificare-geo": {},
      "cordova-plugin-notificare-inbox": {},
      "cordova-plugin-notificare-loyalty": {},
      "cordova-plugin-notificare-push": {},
      "cordova-plugin-notificare-push-ui": {},
      "cordova-plugin-notificare-scannables": {}
    },
    "platforms": [
      "ios",
      "android"
    ]
  }
}
