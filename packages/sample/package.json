{
  "name": "sample",
  "private": "true",
  "displayName": "Sample",
  "version": "1.0.0",
  "description": "A simplified fashion application to demonstrate Notificare basic features.",
  "main": "index.html",
  "scripts": {
    "android": "cordova run android",
    "ios": "cordova run ios",
    "cordova:refresh-plugins": "yarn cordova:remove-platforms && yarn cordova:add-android && yarn cordova:add-ios",
    "cordova:remove-platforms": "rm -rf platforms plugins",
    "cordova:add-android": "cordova platform add android; cd platforms/android; gradle wrapper",
    "cordova:add-ios": "cordova platform add ios; cd platforms/ios; pod install"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Notificare",
  "license": "Apache-2.0",
  "devDependencies": {
    "cordova-android": "^12.0.1",
    "cordova-ios": "^7.0.1",
    "cordova-plugin-dialogs": "^2.0.2",
    "cordova-plugin-notificare": "3.9.0",
    "cordova-plugin-notificare-assets": "3.9.0",
    "cordova-plugin-notificare-geo": "3.9.0",
    "cordova-plugin-notificare-in-app-messaging": "3.9.0",
    "cordova-plugin-notificare-inbox": "3.9.0",
    "cordova-plugin-notificare-loyalty": "3.9.0",
    "cordova-plugin-notificare-push": "3.9.0",
    "cordova-plugin-notificare-push-ui": "3.9.0",
    "cordova-plugin-notificare-scannables": "3.9.0",
    "cordova-plugin-notificare-user-inbox": "3.9.0",
    "ios-deploy": "^1.12.2"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-notificare": {},
      "cordova-plugin-notificare-assets": {},
      "cordova-plugin-notificare-geo": {},
      "cordova-plugin-notificare-in-app-messaging": {},
      "cordova-plugin-notificare-inbox": {},
      "cordova-plugin-notificare-loyalty": {},
      "cordova-plugin-notificare-push": {},
      "cordova-plugin-notificare-push-ui": {},
      "cordova-plugin-notificare-scannables": {},
      "cordova-plugin-notificare-user-inbox": {},
      "cordova-plugin-dialogs": {}
    },
    "platforms": [
      "ios",
      "android"
    ]
  }
}
