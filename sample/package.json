{
  "name": "io.cordova.hellocordova",
  "displayName": "HelloCordova",
  "version": "1.0.0",
  "description": "A sample Apache Cordova application that responds to the deviceready event.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "link-packages": "run-s link-packages:notificare link-packages:notificare-inbox link-packages:notificare-push link-packages:notificare-push-ui",
    "link-packages:notificare": "cordova plugin add ../notificare --link",
    "link-packages:notificare-inbox": "cordova plugin add ../notificare-inbox --link",
    "link-packages:notificare-push": "cordova plugin add ../notificare-push --link",
    "link-packages:notificare-push-ui": "cordova plugin add ../notificare-push-ui --link",
    "unlink-packages": "run-s unlink-packages:notificare unlink-packages:notificare-inbox unlink-packages:notificare-push unlink-packages:notificare-push-ui",
    "unlink-packages:notificare": "cordova plugin remove cordova-plugin-notificare",
    "unlink-packages:notificare-inbox": "cordova plugin remove cordova-plugin-notificare-inbox",
    "unlink-packages:notificare-push": "cordova plugin remove cordova-plugin-notificare-push",
    "unlink-packages:notificare-push-ui": "cordova plugin remove cordova-plugin-notificare-push-ui"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Apache Cordova Team",
  "license": "Apache-2.0",
  "devDependencies": {
    "cordova-android": "^9.1.0",
    "cordova-ios": "^6.2.0",
    "cordova-plugin-notificare": "file:../notificare",
    "cordova-plugin-notificare-inbox": "file:../notificare-inbox",
    "cordova-plugin-notificare-push": "file:../notificare-push",
    "cordova-plugin-notificare-push-ui": "file:../notificare-push-ui",
    "cordova-plugin-whitelist": "^1.3.4",
    "ios-deploy": "^1.11.4",
    "npm-run-all": "^4.1.5"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-notificare": {},
      "cordova-plugin-notificare-inbox": {},
      "cordova-plugin-notificare-push": {},
      "cordova-plugin-notificare-push-ui": {}
    },
    "platforms": [
      "ios",
      "android"
    ]
  }
}