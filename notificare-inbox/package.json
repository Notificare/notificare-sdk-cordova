  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@types/cordova": "^0.0.34",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "cordova-plugin-notificare": "../notificare",
    "eslint": "^7.15.0",
